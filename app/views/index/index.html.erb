
<%= render(:partial => "shared/navigation") %>
<%= render(:partial => "shared/header") %>
<!-- Spa Page -->
<!-- About Section -->
  <section id="about">
    <div class="container">
      <div class="row">
        <div class="about-content col-lg-12">
           <%= render(:partial => "editable-sections/about-section")%>
        </div>
      </div>
    </div>
  </section>

<!-- Renders the Services Section structure along with the partials -->
<%= services_section('Services') do %>
  <!-- Render the panels -->
  <%= render(:partial => 'editable-sections/services/services-panels')%>
<%end%>




<!-- section_for Portfolio Structured layout -->
<%= portfolio_section('Portfolio') do %>
  <!-- nested partials -->
  <!--  Send data from our json file and pass in local variable for it to be interpolated  -->
  <% @data.each do |data| %>
      <%= render(:partial => 'editable-sections/panels/yelpcamp', :locals => {:title => "YelpCamp", :data => data})%>
  <% end %>
<% end %>
